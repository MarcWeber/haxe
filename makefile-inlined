./ast.cmx: ./ast.ml libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o ast.cmx -c ast.ml

./codegen.cmx: ./codegen.ml ./optimizer.cmx ./typeload.cmx ./typecore.cmx ./type.cmx ./genxml.cmx ./common.cmx ./ast.cmx libs/xml-light/xml-light.cmxa libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/xml-light -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/xml-light -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o codegen.cmx -c codegen.ml

./common.cmx: cppo/cppo ./common.ml ./type.cmx ./ast.cmx libs/extc/extc.cmxa libs/extlib/extLib.cmxa libs/xml-light/xml-light.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extc -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/xml-light -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/xml-light -annot -g   -pp '/pr/tasks/haxe-git/cppo/cppo $(if ${BACKEND_neko}, -D BACKEND_neko,)'  -o common.cmx -c common.ml

./dce.cmx: ./dce.ml ./ast.cmx ./common.cmx ./type.cmx libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o dce.cmx -c dce.ml

./gencommon.cmx: cppo/cppo ./gencommon.ml ./type.cmx ./common.cmx ./codegen.cmx ./ast.cmx libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g   -pp '/pr/tasks/haxe-git/cppo/cppo $(if ${BACKEND_neko}, -D BACKEND_neko,)'  -o gencommon.cmx -c gencommon.ml

./genneko.cmx: ./genneko.ml ./type.cmx ./lexer.cmx ./common.cmx ./codegen.cmx ./ast.cmx libs/neko/neko.cmxa libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/neko -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/neko -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o genneko.cmx -c genneko.ml

./genxml.cmx: ./genxml.ml ./type.cmx ./lexer.cmx ./common.cmx ./ast.cmx libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o genxml.cmx -c genxml.ml

./interp.cmx: cppo/cppo ./interp.ml ./typecore.cmx ./type.cmx ./lexer.cmx ./genneko.cmx ./common.cmx ./codegen.cmx ./ast.cmx ./parser.cmx libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/xml-light/xml-light.cmxa libs/neko/neko.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc -I /pr/tasks/haxe-git/libs/xml-light -I /pr/tasks/haxe-git/libs/neko -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc -I /pr/tasks/haxe-git/libs/xml-light -I /pr/tasks/haxe-git/libs/neko -annot -g   -pp '/pr/tasks/haxe-git/cppo/cppo $(if ${BACKEND_neko}, -D BACKEND_neko,)'  -o interp.cmx -c interp.ml

./matcher.cmx: ./matcher.ml ./optimizer.cmx ./codegen.cmx ./typecore.cmx ./type.cmx ./typer.cmx ./common.cmx ./ast.cmx libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o matcher.cmx -c matcher.ml

./main.cmx: cppo/cppo ./main.ml ./dce.cmx ./matcher.cmx ./typer.cmx ./typeload.cmx ./typecore.cmx ./type.cmx ./parser.cmx ./optimizer.cmx ./lexer.cmx ./interp.cmx ./genxml.cmx ./genneko.cmx ./common.cmx ./codegen.cmx ./ast.cmx ./gencommon.cmx libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc -annot -g   -pp '/pr/tasks/haxe-git/cppo/cppo $(if ${BACKEND_neko}, -D BACKEND_neko,)'  -o main.cmx -c main.ml

./optimizer.cmx: ./optimizer.ml ./typecore.cmx ./type.cmx ./parser.cmx ./common.cmx ./ast.cmx libs/extlib/extLib.cmxa libs/extc/extc.cmxa libs/extc/extc.cmxa
	cd ./;  ocamlopt.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o optimizer.cmx -c optimizer.ml

