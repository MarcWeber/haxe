./typecore.cmo: ./typecore.ml ./type.cmo ./common.cmo ./ast.cmo libs/extlib/extLib.cma libs/extc/extc.cma libs/extc/extc.cma
	cd ./;  ocamlc.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o typecore.cmo -c typecore.ml

./typeload.cmo: ./typeload.ml ./typecore.cmo ./type.cmo ./parser.cmo ./optimizer.cmo ./lexer.cmo ./common.cmo ./ast.cmo libs/extlib/extLib.cma libs/extc/extc.cma libs/extc/extc.cma
	cd ./;  ocamlc.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g     -o typeload.cmo -c typeload.ml

./typer.cmo: cppo/cppo ./typer.ml ./typeload.cmo ./typecore.cmo ./type.cmo ./parser.cmo ./optimizer.cmo ./lexer.cmo ./interp.cmo ./genneko.cmo ./common.cmo ./codegen.cmo ./ast.cmo libs/extlib/extLib.cma libs/extc/extc.cma libs/extc/extc.cma
	cd ./;  ocamlc.opt -I /pr/tasks/haxe-git/libs/extlib -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extlib -annot -g   -pp '/pr/tasks/haxe-git/cppo/cppo $(if ${BACKEND_neko}, -D BACKEND_neko,)'  -o typer.cmo -c typer.ml

./lexer.cmo: ./lexer.ml ./ast.cmo libs/extc/extc.cma libs/extc/extc.cma
	cd ./;  ocamlc.opt -I /pr/tasks/haxe-git/libs/extc  -I /pr/tasks/haxe-git/libs/extc  -annot -g     -o lexer.cmo -c lexer.ml

./haxe.byte: libs/extlib/extLib.cma libs/xml-light/xml-light.cma libs/extlib/extLib.cma libs/extc/extc.cma libs/extlib/extLib.cma libs/xml-light/xml-light.cma libs/extlib/extLib.cma libs/extlib/extLib.cma libs/neko/neko.cma libs/extlib/extLib.cma libs/extlib/extLib.cma libs/extlib/extLib.cma libs/extc/extc.cma libs/xml-light/xml-light.cma libs/neko/neko.cma libs/extlib/extLib.cma libs/extlib/extLib.cma libs/extc/extc.cma libs/extlib/extLib.cma libs/extlib/extLib.cma libs/extc/extc.cma libs/extlib/extLib.cma libs/extlib/extLib.cma libs/extlib/extLib.cma libs/extlib/extLib.cma libs/extc/extc.cma ./ast.cmo ./codegen.cmo ./common.cmo ./dce.cmo ./gencommon.cmo ./genneko.cmo ./genxml.cmo ./interp.cmo ./matcher.cmo ./main.cmo ./optimizer.cmo ./parser.cmo ./type.cmo ./typecore.cmo ./typeload.cmo ./typer.cmo ./lexer.cmo
	cd ./; ocamlc.opt -custom -g -o haxe.byte -I /pr/tasks/haxe-git/libs/extlib extLib.cma -cclib libs/extc/extc_stubs.o -cclib -lz libs/xml-light/xml-light.cma -I /pr/tasks/haxe-git/libs/extc extc.cma -I /pr/tasks/haxe-git/libs/neko neko.cma unix.cma str.cma ast.cmo type.cmo common.cmo dce.cmo typecore.cmo lexer.cmo parser.cmo optimizer.cmo typeload.cmo genxml.cmo codegen.cmo genneko.cmo interp.cmo typer.cmo matcher.cmo gencommon.cmo main.cmo

haxe.byte: ./haxe.byte


